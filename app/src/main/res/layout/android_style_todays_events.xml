<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/scrollingBackground">

    <RelativeLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#efe4b0"
        android:id="@+id/relativeLayout3">

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp"

            android:src="@drawable/new_white"
            android:id="@+id/imageView2" />

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/home_icon_pressed"
            android:id="@+id/imageView3"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_alignTop="@+id/imageView4"
            android:layout_toStartOf="@+id/imageView4" />

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/calendar_white"
            android:id="@+id/imageView4"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_alignTop="@+id/imageView5"
            android:layout_centerHorizontal="true" />

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/clock_white"
            android:id="@+id/imageView5"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_alignTop="@+id/imageView6"
            android:layout_toEndOf="@+id/imageView4" />

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/settings_white"
            android:id="@+id/imageView6"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_alignTop="@+id/imageView2"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/add_light"
        android:id="@+id/imageView7"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <RelativeLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:id="@+id/relativeLayout2"
        android:visibility="visible"
        android:layout_below="@+id/relativeLayout3"
        android:layout_alignParentStart="true">

        <ImageView
            android:layout_width="@dimen/event_height"
            android:layout_height="@dimen/event_height"
            android:paddingRight="5dp"
            android:src="@drawable/check_blue"
            android:id="@+id/imageView8"
            android:layout_alignParentTop="true"
            android:layout_toLeftOf="@+id/imageView9"
            android:layout_toStartOf="@+id/imageView9"
            android:visibility="visible" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="10dp"
            android:paddingLeft="5dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Sample Event"
            android:id="@+id/textView15"
            android:layout_gravity="center_horizontal"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_toStartOf="@+id/imageView8"
            android:visibility="visible" />

        <ImageView
            android:layout_width="@dimen/event_height"
            android:layout_height="@dimen/event_height"
            android:paddingRight="5dp"
            android:src="@drawable/more_blue"
            android:id="@+id/imageView9"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:visibility="visible" />
    </RelativeLayout>

    <RelativeLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:id="@+id/relativeLayout4"
        android:visibility="visible"
        android:layout_below="@+id/relativeLayout2"
        android:layout_alignParentStart="true">

        <ImageView
            android:layout_width="@dimen/event_height"
            android:layout_height="@dimen/event_height"
            android:paddingRight="5dp"
            android:src="@drawable/check_blue"
            android:id="@+id/imageView10"
            android:layout_alignParentTop="true"
            android:layout_toLeftOf="@+id/imageView11"
            android:layout_toStartOf="@+id/imageView11"
            android:visibility="visible" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="10dp"
            android:paddingLeft="5dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Sample Event"
            android:id="@+id/textView16"
            android:layout_gravity="center_horizontal"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_toStartOf="@+id/imageView10"
            android:visibility="visible" />

        <ImageView
            android:layout_width="@dimen/event_height"
            android:layout_height="@dimen/event_height"
            android:paddingRight="5dp"
            android:src="@drawable/more_blue"
            android:id="@+id/imageView11"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:visibility="visible" />
    </RelativeLayout>

    <RelativeLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:id="@+id/relativeLayout5"
        android:visibility="visible"
        android:layout_below="@+id/relativeLayout4"
        android:layout_alignParentStart="true">

        <ImageView
            android:layout_width="@dimen/event_height"
            android:layout_height="@dimen/event_height"
            android:paddingRight="5dp"
            android:src="@drawable/check_blue"
            android:id="@+id/imageView12"
            android:layout_alignParentTop="true"
            android:layout_toLeftOf="@+id/imageView13"
            android:layout_toStartOf="@+id/imageView13"
            android:visibility="visible" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="10dp"
            android:paddingLeft="5dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Sample Event"
            android:id="@+id/textView17"
            android:layout_gravity="center_horizontal"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_toStartOf="@+id/imageView12"
            android:visibility="visible" />

        <ImageView
            android:layout_width="@dimen/event_height"
            android:layout_height="@dimen/event_height"
            android:paddingRight="5dp"
            android:src="@drawable/more_blue"
            android:id="@+id/imageView13"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:visibility="visible" />
    </RelativeLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout3"
        android:visibility="visible"
        android:background="#efe4b0"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/relativeLayout5">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/scrollingBackground" >

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:padding="5dp"
                android:src="@drawable/subtract_time_icon"
                android:id="@+id/imageView21" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="12dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/subtract_time_button"
                android:id="@+id/textView24" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/scrollingBackground" >

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:padding="5dp"
                android:src="@drawable/move_up_icon"
                android:id="@+id/imageView20" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="12dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="move up"
                android:id="@+id/textView23" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/scrollingBackground" >

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:padding="5dp"
                android:src="@drawable/move_down_icon"
                android:id="@+id/imageView16" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="12dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="move down"
                android:id="@+id/textView19" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/scrollingBackground" >

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:padding="5dp"
                android:src="@drawable/move_event_to_tomorrow_icon"
                android:id="@+id/imageView17" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="12dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="move to tomorrow"
                android:id="@+id/textView20" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/scrollingBackground" >

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:padding="5dp"
                android:src="@drawable/edit_blue"
                android:id="@+id/imageView18" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="12dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="edit"
                android:id="@+id/textView21" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/scrollingBackground" >

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:padding="5dp"
                android:src="@drawable/delete_all_test_icon"
                android:id="@+id/imageView19" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="12dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="delete"
                android:id="@+id/textView22" />
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>